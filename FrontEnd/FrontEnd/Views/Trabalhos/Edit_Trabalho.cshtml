@model FrontEnd.Models.Trabalhos

<h4>Trabalhos</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit_Trabalho">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Titulo" class="control-label"></label>
                <input asp-for="Titulo" class="form-control" />
                <span asp-validation-for="Titulo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ReferenciaInfo" class="control-label"></label>
                <input asp-for="ReferenciaInfo" class="form-control" />
                <span asp-validation-for="ReferenciaInfo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Tipo" class="control-label"></label>
                <input asp-for="Tipo" class="form-control" />
                <span asp-validation-for="Tipo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Nota" class="control-label"></label>
                <input asp-for="Nota" class="form-control" />
                <span asp-validation-for="Nota" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AdendaProtocolo" class="control-label"></label>
                <input asp-for="AdendaProtocolo" class="form-control" />
                <span asp-validation-for="AdendaProtocolo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Aluno_Id" class="control-label"></label>
                <select asp-for="Aluno_Id" class="form-control" asp-items="ViewBag.Aluno_Id"></select>
                <span asp-validation-for="Aluno_Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Empresa_Id" class="control-label"></label>
                <select asp-for="Empresa_Id" class="form-control" asp-items="ViewBag.Empresa_Id"></select>
                <span asp-validation-for="Empresa_Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
